{
  "name": "server",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf dist && yarn build && ts-node-dev -r tsconfig-paths/register --files ./src/index.ts",
    "build": "tsc",
    "lint": "eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "generate": "graphql-codegen --config codegen.js",
    "migrate": "yarn prisma migrate dev",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.2",
    "@prisma/client": "^3.9.2",
    "apollo-server-express": "^3.6.3",
    "argon2": "^0.28.4",
    "connect-redis": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^16.3.0",
    "ioredis": "^4.28.5",
    "nodemailer": "^6.7.2",
    "twilio": "^3.74.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@types/connect-redis": "^0.0.18",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.17",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.9.0",
    "gen-env-types": "^1.3.2",
    "prisma": "^3.9.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
